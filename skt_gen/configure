#!/bin/bash
source ../spec.txt

if [[ $VERSION = DEVELOP ]]; then
FORMAT='-f \"\%Uuser \%Ssystem \%Eelapsed \%PCPU \(\%Xtext\+\%Ddata \%Mmax\)k\\n\%Iinputs\+\%Ooutputs \(\%Fmajor\+\%Rminor\)pagefaults \%Wswaps \%C\\n\"'
COUNTER='statcounter.js'
else
FORMAT=''
COUNTER='statcounter_dummy.js'
fi

# Do not edit the following line.
perl -i -p -e "s#PERLPATH#$PERLPATH#g; s#SCLINSTALLDIR#$SCLINSTALLDIR#g; s#CGIDIR#$CGIDIR#g; s#CGIURL#$CGIURL#g; s#HTDOCSDIR#$HTDOCSDIR#g; s#LTPROCBINDIR#$LTPROCBINDIR#g; s#SCLURL#$SCLURL#g; s#DEPTURL#$DEPTURL#g; s#TFPATH#$TFPATH#g; s#COUNTER#$COUNTER#g; s#FORMAT#$FORMAT#g; s#LIB_PERL_PATH#$LIB_PERL_PATH#g; s#MODE#$MODE#g; s#TMPPATH#$TMPPATH#g;" noun/gen_noun.pl noun/noun_format_html.pl noun/web_interface/noun_gen.cgi noun/web_interface/noun_gen.html noun/web_interface/noun.html verb/gen_verb.pl verb/verb_format_html.pl verb/web_interface/verb_gen.cgi verb/web_interface/verb_gen.html kqw/gen_kqw.pl kqw/kqw_avy_html.pl kqw/kqw_format_html.pl kqw/web_interface/kqw_gen.cgi kqw/web_interface/kqw_gen.html waxXiwa/gen_noun.pl waxXiwa/gen_waxXiwa.pl waxXiwa/noun_format_html.pl waxXiwa/waxXiwa_avy_html.pl waxXiwa/waxXiwa_format_html.pl waxXiwa/waxXiwa_ind_html.pl waxXiwa/web_interface/waxXiwa_gen.cgi waxXiwa/web_interface/waxXiwa_gen.html waxXiwa/web_interface/noun_gen.cgi waxXiwa/web_interface/waxXiwa_gen.cgi waxXiwa/web_interface/waxXiwa.html Makefile noun/pUraNa.pl noun/saMKyeya.pl noun/saMKyA.pl noun/sarvanAma.pl skt_gen_server_start.pl client_gen.sh client_gen_file.sh skt_gen_server.sh skt_gen_server_stop.pl compounds/web_interface/js/callcgiscripts.js compounds/cnvrt2utf.pl compounds/cnvrt2utfr.pl compounds/Makefile compounds/web_interface/about.html compounds/web_interface/expert.html compounds/web_interface/index.html compounds/web_interface/index_dev.html compounds/web_interface/index_rmn.html compounds/web_interface/start.html compounds/web_interface/aluk.cgi compounds/web_interface/gawi.cgi compounds/web_interface/pUrvanipAwa.cgi compounds/web_interface/pUrvapaxa.cgi compounds/web_interface/samAsAnwa.cgi compounds/web_interface/samAsaprakAra.cgi compounds/web_interface/samasaprakaraexpert.cgi compounds/web_interface/showindex.cgi compounds/web_interface/upasarjana.cgi compounds/web_interface/uwwarapaxa.cgi
