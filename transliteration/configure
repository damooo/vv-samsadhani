#!/bin/bash
source ../spec.txt

if [[ $VERSION = DEVELOP ]]; then
FORMAT='-f \"\%Uuser \%Ssystem \%Eelapsed \%PCPU \(\%Xtext\+\%Ddata \%Mmax\)k\\n\%Iinputs\+\%Ooutputs \(\%Fmajor\+\%Rminor\)pagefaults \%Wswaps \%C\\n\"'
COUNTER='statcounter.js'
else
FORMAT=''
COUNTER='statcounter_dummy.js'
fi

# Do not edit the following line.
perl -i -p -e "s#PERLPATH#$PERLPATH#g; s#SCLURL#$SCLURL#g; s#SCLINSTALLDIR#$SCLINSTALLDIR#g; s#CGIDIR#$CGIDIR#g; s#CGIURL#$CGIURL#g; s#DEPTURL#$DEPTURL#g; s#HTDOCSDIR#$HTDOCSDIR#g; s#TFPATH#$TFPATH#g; s#COUNTER#$COUNTER#g; s#LIB_PERL_PATH#$LIB_PERL_PATH#g;" transliterate.cgi Makefile transliteration.html about.html
